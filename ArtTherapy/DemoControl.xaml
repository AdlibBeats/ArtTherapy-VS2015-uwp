    <Style TargetType="custom:DemoControl">
        <Style.Setters>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="custom:DemoControl">
                        <Grid>
                            <Grid x:Name="RootGrid" Margin="0" Background="{x:Null}" MaxWidth="200"
                                  Visibility="Collapsed">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="Auto" MaxHeight="158"/>
                                    <RowDefinition Height="Auto"/>
                                    <RowDefinition Height="Auto"/>
                                </Grid.RowDefinitions>
                                <controls:ImageEx x:Name="ProductImage" Grid.Row="0" Grid.RowSpan="2" Stretch="Uniform" Background="{x:Null}"
                                    Source="{Binding Text}" IsCacheEnabled="False"
                                    Visibility="Collapsed"/>
                                <ProgressRing x:Name="IsLoadingImageProgress" Grid.Row="0" Grid.RowSpan="2" Height="30" Width="30" BorderBrush="Gray" Foreground="Black"
                                    HorizontalAlignment="Center" VerticalAlignment="Center"
                                    IsActive="{Binding IsLoadingImage}" Background="{x:Null}"/>
                                <Border x:Name="ProductNameBorder" Grid.Row="1" Background="Black" Opacity="0.7" MaxHeight="100"
                                    Visibility="Collapsed"/>
                                <TextBlock x:Name="ProductName" Grid.Row="1" FontSize="13" Margin="4,7" TextAlignment="Left" TextWrapping="Wrap" Foreground="White"
                                    HorizontalAlignment="Left" VerticalAlignment="Center"
                                    Text="{Binding Name, Converter={StaticResource stringProConverter}}"
                                    Visibility="Collapsed"/>
                                <Grid x:Name="ProductPriceBorder" Grid.Row="2" Background="{StaticResource Static.Grid.Background}">
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition/>
                                        <ColumnDefinition Width="Auto"/>
                                    </Grid.ColumnDefinitions>
                                    <TextBlock x:Name="ProductPrice" Grid.Column="0" Margin="4,5" FontSize="18" TextAlignment="Left" TextWrapping="Wrap"
                                        HorizontalAlignment="Left" VerticalAlignment="Top"
                                        Text="{Binding Description}"
                                        Visibility="Collapsed"/>
                                    <ProgressRing x:Name="IsLoadingPricesProgress" Grid.Column="0" Margin="4,5" Width="15" Height="15" BorderBrush="Gray" Foreground="Black"
                                        HorizontalAlignment="Left" VerticalAlignment="Top"
                                        IsActive="{Binding IsLoadingPrices}"/>
                                    <Button x:Name="ProductTrueBuy" Grid.Column="1" Margin="0,5" Width="30" Height="30"
                                        HorizontalAlignment="Right" VerticalAlignment="Top"
                                        Visibility="Collapsed">
                                        <Button.Content>
                                            <Image Stretch="Uniform" Source="/Assets/trolley-cart@3x.png"/>
                                        </Button.Content>
                                    </Button>
                                    <Button x:Name="ProductFalseBuy" Grid.Column="1" Margin="0,5" Width="30" Height="30" IsEnabled="False"
                                        HorizontalAlignment="Right" VerticalAlignment="Top"
                                        Visibility="Visible">
                                        <Button.Content>
                                            <Image Stretch="Uniform" Source="/Assets/icon-empty-shopping-cart@3x.png"/>
                                        </Button.Content>
                                    </Button>
                                </Grid>
                                <Grid x:Name="ProductRemainsBorder" Grid.Row="3" Background="{StaticResource Static.Grid.Background}"
                                      Visibility="Visible">
